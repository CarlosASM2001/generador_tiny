// Programa simple para probar el analizador léxico

global x;
var y : array[5];

function suma(a, b)
begin
    return a + b
end

begin
    x := 10;
    y[0] := x ** 2;
    
    if x > 5 && x <= 20 then
        write "x está en rango"
    end;
    
    for i := 0 to 4 step 1
    begin
        y[i] := i % 3;
        if y[i] != 0 then
            continue
        end
    end
end